{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "skipDefaultLibCheck": true
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts",
    "test/emp"
  ],
  "exclude": [
    "node_modules"
  ],
  "files": [
    "src/server.ts",
  ]
}